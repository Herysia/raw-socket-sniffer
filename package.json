{
  "name": "raw-socket-sniffer",
  "version": "0.1.1",
  "description": "IPv4 packet capture on Windows without a kernel driver.",
  "main": "index.js",
  "scripts": {
    "install": "node-gyp rebuild",
    "rebuild": "node-gyp rebuild",
    "rebuild:electron": "node-gyp rebuild --target=22.0.0 --arch=x64 --dist-url=https://electronjs.org/headers",
    "start:electron": "electron ./repro",
    "start": "node ./repro"
  },
  "keywords": [
    "cap",
    "pcap",
    "wireshark",
    "capture",
    "network",
    "sniffer",
    "ipv4",
    "packet",
    "UDP",
    "TCP"
  ],
  "author": "GirkovArpa",
  "license": "MIT",
  "private": false,
  "gypfile": true,
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GirkovArpa/raw-socket-sniffer"
  },
  "homepage": "https://github.com/GirkovArpa/raw-socket-sniffer#readme",
  "bugs": {
    "url": "https://github.com/GirkovArpa/raw-socket-sniffer/issues",
    "email": "GirkovArpa@protonmail.com"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "electron": "^22.0.0"
  }
}
